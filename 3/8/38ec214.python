with open(INPUT_FILE, "r") as f:
    crabs = [int(num) for num in f.readline().split(",")]

min_cost = float("+inf")
for pos in range(min(crabs), max(crabs) + 1):
    fuel_cost = 0
    for crab in crabs:
        fuel_cost += abs(pos - crab)
    if fuel_cost < min_cost:
        min_cost = fuel_cost
        
print(min_cost)
